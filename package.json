{
  "name": "future-shell",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest --watch",
    "tsc-watch": "tsc -w",
    "start": "tsc-watch --onSuccess ./bin/fs-server",
    "cli": "node dist/startCli.js",
    "generate-api": "tsc-watch --onSuccess 'bin/fs-code-generator --file src/source.graph'"
  },
  "dependencies": {
    "bent": "^1.5.13",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "fast-glob": "^3.0.4",
    "fs-extra": "^8.0.1",
    "ink": "^2.2.0",
    "ink-spinner": "^3.0.1",
    "ink-text-input": "^3.2.0",
    "minimist": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.12.0",
    "source-map-support": "^0.5.12",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.3",
    "concurrently": "^5.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "./src/__tests__/setup.ts"
    ],
    "testMatch": [
      "**/src/**/__tests__/**/*.test.(js|ts)",
      "**/src/**/real-world-test/*.test.(js|ts)",
      "**/src/**/grind-test/*.test.(js|ts)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
