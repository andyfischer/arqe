#! /usr/bin/env node

const { runStandardProcess, runAsync } = require('..')

runStandardProcess(async (graph) => {

    const rels = await runAsync(graph, 'get touchpoint output tagValue');

    for (const rel of rels) {
        console.log(rel
                    .addTag('from(' + rel.getTagValue('tagValue') + '/*)')
                    .removeType('tagValue')
                    .addTag('var')
                    .stringify())
    }
});
